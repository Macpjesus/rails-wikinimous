<div>
  <div>
    <h1>Articles</h1>
    <h2> <%= link_to "Create a new article", new_article_path %> </h2>
  </div>
  <ol>
    <% @articles.each do |article| %>
      <div>
        <li> <%= link_to article.title, article_path(article) %> </li>
        <p> <%= article.content %></p>
    </div>
    <% end %>
  </ol>

</div>
